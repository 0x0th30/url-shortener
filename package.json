{
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/parser": "^7.20.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/md5": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "babel-preset-minify": "^0.5.2",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.2.0",
    "prisma": "^4.5.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "*"
  },
  "dependencies": {
    "@prisma/client": "4.5.0",
    "express": "^4.18.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "winston": "^3.8.2"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "sca": "yarn audit",
    "lint": "eslint --fix src tests",
    "test": "jest --silent --verbose",
    "migrate": "prisma generate && prisma migrate deploy",
    "start:dev": "yarn migrate && ts-node-dev -r tsconfig-paths/register src/app.ts",
    "start:prod": "ls && yarn migrate && node dist/app.js"
  }
}
